import os

DefaultEnvironment(tools=[])

environment = Environment(
    tools=['link','ar' ,'{}']
)

o1 = environment.Object('source/helloWorld.d',DINTFDIR = "../include")[0]
o2 = environment.Object('helloWorldMain.d',DPATH="include")

environment.Program('hw', [o2,o1])
